<!DOCTYPE html>
<html>
  <head>
  	<meta charset="UTF-8">
	<!-- IE Edge Meta Tag -->
	<meta http-equiv="X-UA-Compatible" content="IE=edge">

	<!-- Viewport -->
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<!-- Minified CSS -->
	<link rel="stylesheet" href="assets/stylesheets/bs/css/bootstrap.min.css">


	<!-- Optional IE8 Support -->
	<!--[if lt IE 9]>
	<script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
	<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->

    <title>APix</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

  <nav class="navbar navbar-default">
  	<div class="container">
  		 <ul class="nav navbar-nav">
				<li><%= link_to('Home', {:controller => 'visitor', :action => 'index'}) %></li>
				<li><%= link_to('Subscriber', {:controller => 'subscriber', :action => 'index'}) %></li>
				<li><%= link_to('Admin', {:controller => 'admin', :action => 'index'}) %></li>
				<li><%= link_to('Super Admin', {:controller => 'superadmin', :action => 'index'}) %></li>
				<li><%= link_to('Investors', {:controller => 'investor', :action => 'index'}) %></li>
			</ul>
		</div>  	
  </nav>
 
  <div class="container">
  	<div class="row">
  		<%= yield %>
  	</div>
  </div>

    <!-- jQuery -->
	<script src="assets/javascripts/bs/jquery-3.1.1.min.js"></script>

	<!-- Minified JavaScript -->
	<script src="assets/javascripts/bs//bootstrap.min.js"></script>
  </body>
</html>
